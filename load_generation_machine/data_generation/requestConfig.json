{
    "options": {
        "target_utilization": 0.25,
        "start_rate": 500
    },
    "services": {
        "persistence": {
            "categories/": [
                {},
                {
                    "path": "{category_id}"
                }
            ],
            "orders/": [
                {
                    "path": "user/{user_id}"
                },
                {
                    "method": "POST",
                    "body": "order"
                }
            ],
            "orderitems/": [
                {
                    "method": "POST",
                    "body": "orderItem"
                }
            ],
            "products/": [
                {
                    "path": "{product_id}"
                },
                {
                    "path": "category/{category_id}?start={start}&max={max}"
                },
                {
                    "path": "count/{category_id}"
                }
            ],
            "users/": [
                {
                    "path": "name/user{user_num}"
                }
            ]
        },
        "auth": {
            "useractions/": [
                {
                    "method": "POST",
                    "path": "placeorder?{order_details}",
                    "body": "blob"
                },
                {
                    "method": "POST",
                    "path": "login?name=user{user_num}&password=password",
                    "body": {}
                },
                {
                    "method": "POST",
                    "path": "logout",
                    "body": "blob"
                },
                {
                    "method": "POST",
                    "path": "isloggedin",
                    "body": "blob"
                }
            ],
            "cart/": [
                {
                    "method": "POST",
                    "path": "add/{product_id_not_in_cart}",
                    "body": "blob"
                },
                {
                    "method": "POST",
                    "path": "remove/{product_id_in_cart}",
                    "body": "blob"
                },
                {
                    "method": "PUT",
                    "path": "{product_id_in_cart}?quantity={quantity}",
                    "body": "blob"
                }
            ]
        },
        "recommender": {
            "recommend?": [
                {
                    "method": "POST",
                    "path": "{user_id}",
                    "body": "orderItemArray"
                }
            ]
        },
        "image": {
            "image/": [
                {
                    "method": "POST",
                    "path": "getProductImages",
                    "body": "productImages"
                },
                {
                    "method": "POST",
                    "path": "getWebImages",
                    "body": "webImages"
                }
            ]
        },
        "webui": {
            "cartAction?": [
                {
                    "path": "addToCart=&productid={product_id}"
                },
                {
                    "path": "removeProduct={product_id_in_cart}"
                },
                {
                    "path": "cartupdateCartQuantities="
                },
                {
                    "path": "proceedtoCheckout="
                },
                {
                    "path": "firstname=F&lastname=L&adress1=A&adress2=A&cardtype=C&cardnumber=1&expirydate=12/2050&confirm=Confirm"
                }
            ],
            "cart/": [
                {}
            ],
            "category?": [
                {
                    "path": "category={category_id}&page="
                }
            ],
            "index/": [
                {}
            ],
            "loginAction?": [
                {
                    "path": "username=user{user_num}&password=password"
                },
                {
                    "path": "logout="
                }
            ],
            "order/": [
                {}
            ],
            "product?": [
                {
                    "path": "id={product_id}"
                }
            ],
            "profile/": [
                {}
            ]
        }
    }
}
